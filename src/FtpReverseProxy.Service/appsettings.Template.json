{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "FtpReverseProxy": "Information"
    }
  },
  "Proxy": {
    "Ftp": {
      "Enabled": true,
      "ListenAddress": "0.0.0.0",
      "Port": 21
    },
    "FtpsImplicit": {
      "Enabled": false,
      "ListenAddress": "0.0.0.0",
      "Port": 990
    },
    "Sftp": {
      "Enabled": false,
      "ListenAddress": "0.0.0.0",
      "Port": 22,
      "HostKeyPath": null,
      "HostKeyPassword": null
    },
    "DataChannel": {
      "MinPort": 50000,
      "MaxPort": 51000,
      "ExternalAddress": null
    },
    "TlsCertificate": {
      "Path": "/path/to/your/certificate.pfx",
      "Password": "your-certificate-password"
    },
    "Database": {
      "Provider": "PostgreSQL",
      "ConnectionString": "Host=localhost;Database=ftpproxy;Username=postgres;Password=your-password"
    },
    "Redis": {
      "Enabled": false,
      "ConnectionString": "localhost:6379",
      "CacheTtlSeconds": 300
    }
  }
}
