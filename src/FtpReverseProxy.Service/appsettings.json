{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "FtpReverseProxy": "Debug"
    }
  },
  "Proxy": {
    "Ftp": {
      "Enabled": true,
      "ListenAddress": "0.0.0.0",
      "Port": 21
    },
    "FtpsImplicit": {
      "Enabled": false,
      "ListenAddress": "0.0.0.0",
      "Port": 990
    },
    "Sftp": {
      "Enabled": false,
      "ListenAddress": "0.0.0.0",
      "Port": 22,
      "HostKeyPath": null,
      "HostKeyPassword": null
    },
    "DataChannel": {
      "MinPort": 50000,
      "MaxPort": 51000,
      "ExternalAddress": null
    },
    "TlsCertificate": null,
    "Database": {
      "Provider": "PostgreSQL",
      "ConnectionString": ""
    },
    "Redis": {
      "Enabled": false,
      "ConnectionString": "localhost:6379",
      "CacheTtlSeconds": 300
    }
  }
}
